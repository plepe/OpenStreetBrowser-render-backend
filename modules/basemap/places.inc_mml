      <Parameter name="password">%DB_PASS%</Parameter>
      <Parameter name="geometry_field">way</Parameter>
      <Parameter name="srid">%DB_SRID%</Parameter>
      <Parameter name="table">
      (select /* place high */ /*layer* clear_label_cache="on" */
